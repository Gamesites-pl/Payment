        "symfony/form": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "^8.3"
    },
    "autoload": {
        "psr-4": {
